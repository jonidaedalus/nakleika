<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<window xmlns="http://schemas.haulmont.com/cuba/screen/window.xsd"
        caption="msg://DCarModelEdit.caption"
        focusComponent="form"
        messagesPack="com.company.nakleika.web.screens.dcarmodel">
    <data>
        <instance id="dCarModelDc"
                  class="com.company.nakleika.entity.dict.DCarModel">
            <view extends="dCarModel-view"/>
            <loader/>
        </instance>
    </data>
    <dialogMode height="600"
                width="800"/>
    <layout expand="editActions" spacing="true">
        <form id="form" dataContainer="dCarModelDc">
            <column width="350px">
                <lookupField id="brandField" property="brand"/>
                <textField id="nameField" property="name"/>
            </column>
        </form>
        <hbox id="editActions" spacing="true">
            <button id="commitAndCloseBtn" action="windowCommitAndClose"/>
            <button id="closeBtn" action="windowClose"/>
        </hbox>
    </layout>
</window>
